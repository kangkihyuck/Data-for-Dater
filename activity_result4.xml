<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#FFFFFF"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity4">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="40dp">
        <TextView
            android:background="#FFE691AF"
            android:gravity="center"
            android:text="DATAR"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/MainTItleTextView7"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="30sp"
            android:textStyle="bold" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
            android:clickable="true"/>
        <Spinner
            android:layout_width="28dp"
            android:layout_height="26dp" android:id="@+id/resultSpinner2"
            android:background="@drawable/listwhite"
            app:layout_constraintEnd_toEndOf="@+id/MainTItleTextView7"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" android:layout_marginTop="4dp"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:text="로그인"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/MainLoginTextView7"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="@+id/MainTItleTextView7"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:clickable="true" android:textSize="18sp"/>
        <TextView
            android:layout_width="22dp"
            android:layout_height="22dp" android:id="@+id/homeButton7"
            app:layout_constraintStart_toEndOf="@+id/MainLoginTextView7" android:layout_marginLeft="12dp"
            android:layout_marginStart="12dp" android:background="@drawable/homewhite"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    </android.support.constraint.ConstraintLayout>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content"
            android:orientation="vertical">
            <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultTitleTextView4"
                android:gravity="center"
                android:textAppearance="@style/TextAppearance.AppCompat.Large" android:textSize="40sp"
                android:layout_margin="10dp"/>
            <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/averageRatingTextView4"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"
                android:layout_margin="10dp" android:gravity="center"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="181dp" android:layout_margin="10dp">
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent" app:srcCompat="@mipmap/ic_launcher"
                    android:id="@+id/resultImageView4" android:layout_weight="1" android:layout_margin="10dp"/>
            </LinearLayout>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/contentTypeTextView4"
                android:layout_margin="10dp" android:textAppearance="@style/TextAppearance.AppCompat"
                android:textSize="18sp"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultAddressTextView4"
                android:layout_margin="10dp"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultTelephoneTextView4"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"
                android:layout_margin="10dp" android:clickable="true"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultExpguideTextView4"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"
                android:layout_margin="10dp" android:clickable="false"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultParkingTextView4"
                android:layout_margin="10dp"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultUsetimeTextView"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"
                android:layout_margin="10dp"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultRestdateTextView4"
                android:layout_margin="10dp"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"/>
            <TextView
                android:text="TextView"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/resultChkcreditcardTextView4"
                android:textAppearance="@style/TextAppearance.AppCompat" android:textSize="18sp"
                android:layout_margin="10dp"/>

            <android.support.constraint.ConstraintLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <TextView
                    android:clickable="true"
                    android:text="지도로 위치 보기"
                    android:layout_width="130dp"
                    android:layout_height="wrap_content" android:id="@+id/locationTextView4"
                    android:contentDescription="mapTitleTextView"
                    android:textAppearance="@style/TextAppearance.AppCompat" android:layout_margin="10dp"
                    android:textStyle="bold" android:textSize="18sp" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="8dp"
                    app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                    android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
                <ToggleButton
                    android:text="ToggleButton"
                    android:textOn=""
                    android:textOff=""
                    android:layout_width="29dp"
                    android:layout_height="42dp" android:id="@+id/toggleButton4" android:layout_weight="1"
                    android:layout_margin="10dp"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:background="@drawable/ic_arrow_drop_down_black_24dp"
                    app:layout_constraintVertical_bias="0.2"
                    app:layout_constraintStart_toEndOf="@+id/locationTextView4" android:layout_marginStart="8dp"
                    android:layout_marginLeft="8dp"/>
            </android.support.constraint.ConstraintLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:visibility="gone" android:id="@+id/goneLayout4">
                <fragment
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:id="@+id/map"
                    tools:context=".LocationActivity"
                    android:name="com.google.android.gms.maps.SupportMapFragment" android:layout_margin="10dp"/>
            </LinearLayout>
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="40dp">

                <TextView
                    android:text="댓글 목록"
                    android:layout_width="78dp"
                    android:layout_height="22dp" android:id="@+id/comentTextView4"
                    android:textAppearance="@style/TextAppearance.AppCompat" android:layout_margin="10dp"
                    android:textStyle="bold" android:textSize="18sp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toTopOf="parent"/>

                <ToggleButton
                    android:text="ToggleButton"
                    android:textOn=""
                    android:textOff=""
                    android:layout_width="27dp"
                    android:background="@drawable/ic_arrow_drop_down_black_24dp"
                    android:layout_height="wrap_content" android:id="@+id/replyToggleButton4"
                    tools:layout_editor_absoluteY="0dp" app:layout_constraintStart_toEndOf="@+id/comentTextView4"
                    android:layout_marginLeft="8dp" android:layout_marginStart="8dp"/>
            </android.support.constraint.ConstraintLayout>
            <android.support.v7.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="350dp" android:id="@+id/recycler_view_content2"
                android:layout_margin="10dp" android:visibility="gone"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" android:id="@+id/replyLayout2" android:visibility="gone">
                <RatingBar
                    android:id="@+id/ratingBar2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:isIndicator="false"
                    android:numStars="5"
                    android:rating="0"
                    android:stepSize="1"
                    android:max="5" android:layout_margin="10dp" android:layout_gravity="center_horizontal"/>
                <EditText
                    android:background="@drawable/edittext_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/titleEditText2" android:hint="댓글 제목을 입력하세요." android:layout_margin="10dp"/>
                <EditText
                    android:background="@drawable/edittext_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textPersonName"
                    android:ems="10"
                    android:id="@+id/contentEditText2" android:hint="댓글 내용을 입력하세요." android:layout_margin="10dp"/>

                <Button
                    android:id="@+id/replyResgierButton2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:background="@color/colorPrimaryDark"
                    android:shadowRadius="16"
                    android:text="등    록"
                    android:textColor="@color/common_google_signin_btn_text_dark_default" />

                <Button
                    android:id="@+id/replyUpdateButton2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:background="@color/colorPrimaryDark"
                    android:shadowRadius="16"
                    android:text="수    정"
                    android:textColor="@color/common_google_signin_btn_text_dark_default"
                    android:textSize="18sp"
                    android:visibility="gone" />
            </LinearLayout>
            <TextView
                android:text="댓글을 달기 위해서는 로그인을 해야 합니다."
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/replyAlarmTextView2"
                android:visibility="visible" android:textSize="18sp" android:layout_margin="10dp"
                android:clickable="true"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>